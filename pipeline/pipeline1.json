{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "mvnet_copy",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "If Condition1",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
